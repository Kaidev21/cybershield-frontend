{
  "name": "cybershield-frontend",
  "version": "2.2.0",
  "private": true,
  "type": "module",
  "description": "Plateforme SaaS de Cyberdéfense managée pour PME/TPE",
  "author": "Yann Landry Kaïssa Same Junior",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
