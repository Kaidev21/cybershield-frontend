    "vue-chartjs": "^5.3.0",
    "lucide-vue-next": "^0.300.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3"
  },
  "keywords": [
    "cybersecurity",
    "saas",
    "vulnerability-scanner",
    "security-platform",
    "vue3",
